! xscreensaver ---------------------------------------------------------------

!font settings
xscreensaver.Dialog.headingFont:        -*-ohsnap.icons-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.bodyFont:           -*-ohsnap.icons-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.labelFont:          -*-ohsnap.icons-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.unameFont:          -*-ohsnap.icons-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.buttonFont:         -*-ohsnap.icons-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.Dialog.dateFont:           -*-ohsnap.icons-medium-r-*-*-12-*-*-*-*-*-*-*
xscreensaver.passwd.passwdFont:         -*-ohsnap.icons-medium-r-*-*-12-*-*-*-*-*-*-*

!xscreensaver.Dialog.headingFont:        -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
!xscreensaver.Dialog.bodyFont:           -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
!xscreensaver.Dialog.labelFont:          -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
!xscreensaver.Dialog.unameFont:          -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
!xscreensaver.Dialog.buttonFont:         -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
!xscreensaver.Dialog.dateFont:           -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*
!xscreensaver.passwd.passwdFont:         -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*

!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         #c2c2b0
xscreensaver.Dialog.background:         #191919
xscreensaver.Dialog.topShadowColor:     #292929
xscreensaver.Dialog.bottomShadowColor:  #292929
xscreensaver.Dialog.Button.foreground:  #191919
xscreensaver.Dialog.Button.background:  #c2c2b0
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #191919
xscreensaver.Dialog.text.background:    #c2c2b0
xscreensaver.Dialog.internalBorderWidth:10
xscreensaver.Dialog.borderWidth:        1
xscreensaver.Dialog.shadowThickness:    1
!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #87877b
xscreensaver.passwd.thermometer.background:  #000000
xscreensaver.passwd.thermometer.width:       10
!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %a %b %d, %Y

Xcursor.theme: Mac_OSX_Lion
