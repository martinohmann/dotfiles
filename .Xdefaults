! common config
URxvt.termName: rxvt-unicode-256color
URxvt.saveLines: 5000
URxvt.scrollBar: false
URxvt.secondaryScroll: true
URxvt.font: xft:Hack Nerd Font Mono:pixelsize=11
!URxvt.geometry: 78x27
!URxvt.internalBorder: 2
!URxvt.inheritPixmap: true 
!URxvt.tintColor: Blue
!URxvt.transparent: true
!URxvt.shading: 10
URxvt.depth: 32

! perl extensions
URxvt.perl-ext: default,url-select
URxvt.perl-ext-common: default,matcher,clipboard,resize-font

! disable default Ctrl+Shift-Binding
URxvt.iso14755: False
URxvt.iso14755_32: False

! url launcher
URxvt.urlLauncher: chromium
URxvt.url-launcher: /usr/bin/chromium
URxvt.matcher.button: 2

! home + end key
URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~

! url launcher (Alt+u)
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/chromium
URxvt.url-select.underline: true

! secondary clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

